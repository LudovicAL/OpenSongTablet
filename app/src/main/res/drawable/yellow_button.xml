<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#cccc12" />
      <stroke
          android:width="1dp"
          android:color="#666612" />
      <corners
          android:radius="10dp" />
      <padding
          android:left="20dp"
          android:top="20dp"
          android:right="20dp"
          android:bottom="20dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#cccc12"
          android:endColor="#666612"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#666612" />
      <corners
          android:radius="10dp" />
      <padding
          android:left="20dp"
          android:top="20dp"
          android:right="20dp"
          android:bottom="20dp" />
    </shape>
  </item>
</selector>